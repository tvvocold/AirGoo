{
    "listen_address": "{OPENSHIFT_NODEJS_IP}",

    "listen_port": "{OPENSHIFT_NODEJS_PORT}",

    "target_google": {
        "host": "gist",
        "domain": "github.com"
    },

    "force_https": true,

    "backend_https": true,

    "server_header": "AirGoo",

    "force_cached_time": 0,

    "gzip_r4": true,

    "logging": true,

    "trust_proxy": true,

    "prefer_lang": "zh-CN",

    "max_transmit_size": 10485760,

    "ext_domains": [
        ".github.com",
        ".google-analytics.com",
        ".githubapp.com",
        ".githubusercontent.com"
        
    ]
}